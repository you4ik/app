{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.+\\.css)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/(.+\\.js)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }
  